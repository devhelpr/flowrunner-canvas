    "gulp-watch": "5.0.1",
    "jest": "^24.5.0",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "ts-loader": "6.0.4",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.4000",
    "vinyl-named": "^1.1.0",
    "webpack-stream": "^5.2.1"
