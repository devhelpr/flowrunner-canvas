{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/flowrunner-canvas-icons/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "libs/flowrunner-canvas-icons/lib",
        "tsConfig": "libs/flowrunner-canvas-icons/tsconfig.lib.json",
        "project": "libs/flowrunner-canvas-icons/package.json",
        "entryFile": "libs/flowrunner-canvas-icons/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/flowrunner-canvas-icons/**/*.svg",
            "input": ".",
            "output": "."
          }
        ],
        "styles": []
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/flowrunner-canvas-icons/**/*.ts"
        ]
      }
    }
  },
  "tags": []
}
