.taskbar {
	touch-action: auto;
}

.taskbar__ribbon {
	touch-action: auto;
	display: flex;
	flex-direction: column;		
	flex: 1 1 100%;
	width: 100%;
	padding-left: 10px;
}

.taskbar__ribbon::before {
	content: "";
	display: block;
	width: 8px;
	position: fixed;
	top: 75px;
	bottom: 5px;
	left: 2px;
	background-image: linear-gradient(0deg, #ffffff 20%, #999999 20%, #999999 50%, #ffffff 50%, #ffffff 70%, #999999 70%, #999999 100%);
	background-size: 10.00px 10.00px;
	border: 1px #999999 solid;
	border-radius: 4px;
	touch-action: pan-y;
	filter: drop-shadow(-4px 4px 6px #808080);
}

@media only screen and (max-width: 1023px) {

	.taskbar {
		bottom: 56px;
		height: auto;
		display: none;

		display: flex;
		width: 100%;
		height: 56px;
		flex-direction: row;
		overflow-x: scroll;

		touch-action: auto;
		overflow-y: hidden;
		overscroll-behavior-y: none;
	}

    .taskbar__ribbon {
		display: flex;
		flex-direction: row;		
		flex: 1 1 auto;
		width: auto;
		padding-left: 0;
		margin-top: 13px;
		overscroll-behavior-y: none;	
	}

	.taskbar__ribbon::before {
		content: "";
		display: block;
		height: 12px;
		width: auto;
		position: fixed;
		top: 74px;
		left: 2px;
		right: 2px;
		background-image: linear-gradient(90deg, #ffffff 20%, #999999 20%, #999999 50%, #ffffff 50%, #ffffff 70%, #999999 70%, #999999 100%);
		background-size: 10.00px 10.00px;
		border-radius: 4px;
		touch-action: pan-x;		
	}

	.taskbar__task {
		width: 128px;
		text-overflow: ellipsis;
		margin-right: 5px;
	}

	.taskbar__taskname {
		margin-right: 5px;
	}

	.ui-controls-bar {
		bottom: 56px;
		height: auto;
	}

	.toolbar__container .navbar-collapse {
		display: flex !important;
	}

	.toolbar__container .form-inline {
		flex-direction: row;
		overflow-x: scroll;	
		width: 100%;	
	}

	.toolbar__container .form-inline::-webkit-scrollbar {
		display: none;
	}
}

