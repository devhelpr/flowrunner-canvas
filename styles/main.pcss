.layout__dropzone {
	overflow-y: auto;
	height: calc(100% - 64px);
	overflow-x: hidden;
	scroll-behavior: smooth;
	overscroll-behavior-y: contain;

}

.layout__draggables {
	overflow-y: auto;
	height: calc(100% - 64px);
	overflow-x: hidden;
	scroll-behavior: smooth;
	overscroll-behavior-y: contain;
}

