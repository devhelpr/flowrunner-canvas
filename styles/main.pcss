* {
	box-sizing: border-box;
}

body, html {
	height: 100vh;
	max-height: 100vh;
	min-height: 100vh;
	overflow: hidden;
    overscroll-behavior-x: none;
}

html {
	height: -webkit-fill-available;
}

body {
	min-height: -webkit-fill-available;
}

img {
	max-width: 100%;
}

#loading {
	position: absolute;
	left: 0;
	top: 0;
	width: 100vw;
	height: 100%;
	z-index: 100000;
	background: #f2f2f2;
	opacity: 1;
	transition: opacity 0.3s ease-in-out;
	display: flex;
	align-items: center;
	justify-content: center;
}

#loading.loaded {
	opacity: 0;
}
#loading.hidden {
	display: none;
	z-index: -1;
}

.text-white a,
.text-white a:hover {
	color: #ffffff;
}

.modal {
	z-index: 10050;
}

.modal-backdrop.fade {
	z-index: 10040;
}

.toolbar__root {
	height: 72px;
	position: absolute;
    top: 0;
    height: 72px;
    z-index: 10000;
	width: 100%;
}

.sticky-top.toolbar__root {
	z-index: 10000 !important;
}

.toolbar__container {
	height: 72px;
	padding-top: 8px;
}

.stage-container {
	overflow: hidden;
	height: 100%;
	height: 100%;
}

.konvajs-content {
	margin: 0 auto;
	box-shadow: 0px 5px 10px rgba(0,0,0,0.5);
	box-shadow:none;
	margin-bottom: 20px;
	background: #e2e2e2;
}

.stage-container {

	transform: translateZ(0);
	backface-visibility: hidden;
	perspective: 1000;
	transform: translate3d(0, 0, 0);
}
.stage-container canvas,
.stage-container .konvajs-content{
	transform: translateZ(0);
	backface-visibility: hidden;
	perspective: 1000;
	transform: translate3d(0, 0, 0);
}
.stage-container.konvajs-content {
	min-height: 100% !important;
}
.canvas-controller {
	width:100%;
	height:calc(100% - 112px);
}

.canvas-controller__scroll-container {
	width: 100%;
	overflow: scroll;
	overflow:hidden;
	-webkit-overflow-scrolling: touch;
	transition: opacity .15s ease-in-out;
	position: relative;
	z-index: 1;
	height: calc(100%);
}


.canvas__html-elements {
	position: absolute;
	left: 0;
	top: 0;
	width:100%;
	height:calc(100%);
	z-index: 2;
	pointer-events: none;
	
	transform: translateZ(0);
	backface-visibility: hidden;
	perspective: 1000;
	transform: translate3d(0, 0, 0);
}

.canvas__html-shape {
	position: absolute;
	border-radius: 10px;
	will-change: transform;
}

.canvas__html-shape-thumb-start {
	width: 16px;
    height: 16px;
    background: transparent;
    position: absolute;
    right: 0px;
    top: 44px;
	z-index: 2;
	pointer-events: none;
	cursor: pointer;

	width: 0px;
    height: 0px;
    border-top: 12px solid transparent;
    border-bottom: 12px solid transparent;
	border-right: 12px solid #c2c2c2;
	
	width: 16px;
    height: 16px;
	border-radius: 50%;
	right: -8px;
	border: 2px #000000 solid;
	background: #ffffff;
}
.canvas__html-shape-thumb-start::before {
	content: "";
    position: absolute;
    top: -4px;
	z-index: 2;
	
	width: 20px;
    height: 20px;
	border-radius: 50%;
	right: -4px;
	border: 2px #e2e2e2 solid;
	background: transparent;
}

.canvas__html-shape-thumb-end {
	width: 16px;
    height: 16px;
    background: transparent;
    position: absolute;
	left: 0px;
    top: 44px;
	z-index: 2;
	pointer-events: none;
	cursor: pointer;

	width: 0px;
    height: 0px;
    border-top: 12px solid transparent;
    border-bottom: 12px solid transparent;
	border-left: 12px solid #c2c2c2;
	
	width: 16px;
	height: 16px;
	left: -8px;
	border-radius: 50%;
	border: 2px #000000 solid;
	background: #ffffff;
}

.canvas__html-shape-thumb-end::before {
	content: "";
    position: absolute;
    top: -4px;
	z-index: 2;
	
	width: 20px;
    height: 20px;
	border-radius: 50%;
	right: -4px;
	border: 2px #e2e2e2 solid;
	background: transparent;
}

.canvas__html-shape-event {
	width: 16px;
    height: 16px;
    background: transparent;
    position: absolute;
	right: 0px;
    top: 44px;
    z-index: 2;

	border-top: 12px solid transparent;
    border-bottom: 12px solid transparent;
	border-right: 12px solid #a000a0;
	
	width: 16px;
	height: 16px;
	right: -8px;
	border-radius: 50%;
	border: 0 transparent none;
	background: #a000a0;
}


.canvas__html-shape-0 {
	top: 44px;
}

.canvas__html-shape-1 {
	top: 68px;
}

.canvas__html-shape-2 {
	top: 92px;
}

.canvas__html-shape--in-bar {
	position: relative;
	margin-bottom: 10px;

	border-radius: 10px;
	overflow: hidden;
	width: 100%;
}

.html-plugin-node {
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	padding: 20px;
	height: 100%;
	width: 100%;

	margin: 0 auto;
	position: relative;

	transition: filter 0.15s ease-in-out, background-color 0.15s ease-in-out;
	border-bottom-right-radius: 8px;
    border-bottom-left-radius: 8px
}

.html-plugin-node__visibility {
	position: absolute;
    left: 20px;
    top: 15px;
    opacity: .5;
}

.html-plugin-node__h-100 {
	display: flex;
    flex-direction: column;
    justify-content: stretch;
    align-items: stretch;
    padding: 10px;
    height: calc(100%);
    width: 100%;
    margin: 0 auto;
    flex: 1 0 100%;
    position: absolute;
    top: 0;
    left: 0;
}

.html-plugin-node--animatedgridcanvashtml-plugin-node__h-100 {
	padding: 10px;
}

.html-plugin-node__grid-edit {
	justify-content: flex-start;
	cursor: default;
}

.html-plugin-node--wrap {
	white-space: pre;
}

.canvas__html-shape {
	display: flex;
	flex-direction: column;
}

.canvas__html-shape-bar {
	position: relative;
	pointer-events: none;
	height: 26px;
	min-height: 26px;
	display: flex;
	flex-direction: row;
	justify-content: flex-start;
	align-items: center;
	background: #c2c2c2;
	border-top-left-radius: 8px;
	border-top-right-radius: 8px;	
}

.canvas__html-shape-bar-title {
	margin-left: 10px;
}

.canvas__html-shape-title-icon {
	margin-right: 10px;
}

.canvas__html-shape-bar-icon {	
	margin-right: 10px;
	color: #000000;
	pointer-events: auto;
}

.canvas__html-shape-bar-icon:first-of-type {
	margin-left: auto;
}

.connecting-nodes .canvas__html-shape-bar-icon {
	pointer-events: none;
}

.canvas__html-shape-bar-icon:hover {
	text-decoration: none;
}

.canvas__html-shape-bar--selected {
	background: #a2a2a2;
}

.canvas__html-shape-body {
	position: relative;
	pointer-events: none;
	min-height: 100%;
	height: auto;
	width: 100%;

	flex: 1 1 100%;
    background: white;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.connecting-nodes .canvas__html-shape-body {
	pointer-events: none;
}

[data-html-plugin="debugNode"][data-visualizer="default"] .html-plugin-node {
	/*white-space: normal;*/
	
	white-space: normal;
    word-break: break-word;
}

[data-html-plugin="dataGridNode"] .canvas__html-shape-body,
[data-html-plugin="debugNode"][data-visualizer="default"] .canvas__html-shape-body,
[data-html-plugin="formNode"] .canvas__html-shape-body {
	align-items: flex-start;
	overflow: visible;
	background: transparent;
}

[data-html-plugin="dataGridNode"] .html-plugin-node,
[data-html-plugin="debugNode"][data-visualizer="default"] .html-plugin-node,
[data-html-plugin="formNode"] .html-plugin-node {
	height: auto;
	min-height: 100%;
}

[data-html-plugin="dataGridNode"] .form,
[data-html-plugin="debugNode"][data-visualizer="default"] .form,
[data-html-plugin="formNode"] .form {
	padding: 0 5px;
}

[data-html-plugin="formNode"] .html-plugin-node {
	justify-content: flex-start;
}

.mouse--moving .canvas__html-shape-body {
	pointer-events: none;
}

.html-plugin-node {
	pointer-events: none;
	z-index: 1;
}
.html-plugin-node > div {
	height: 100%;
	pointer-events: auto;
}


.canvas-controller__scroll-container .html-plugin-node::before {
    content: "";
    display: block;
    position: absolute;
    top: -27px;
    left: 0px;
    width: 100%;
    height: calc(100% + 29px);
    border: 2px transparent solid;
    border-radius: 8px;
	transition: border 0.2s ease-in-out;
	z-index: 2;
}


.canvas-controller__scroll-container .form .html-plugin-node::before {
	display: none;
}

.has-error .html-plugin-node::before {
	border: 2px #e00000 solid;
}

.html-plugin-node .form {
	pointer-events: auto;
}

[data-html-plugin="executeNode"] .html-plugin-node a {
	pointer-events: auto;
}

.html-plugin-node > div.stage-div {
	height: auto;
}

.connecting-nodes .html-plugin-node > div {
	pointer-events: none;
}

.mouse--moving .html-plugin-node > div {
	pointer-events: none;
}

.connecting-nodes .html-plugin-node .form {
	pointer-events: none;
}

.mouse--moving .html-plugin-node .form {
	pointer-events: none;
}


.html-plugin-node .konvajs-content {
	margin-bottom: 0;
	background: #ffffff;
}

.overflow-y-scroll {
	overflow-y: scroll;
}

.taskbar {
	position: absolute;
    top: 72px;
	width: 48px;
	width: 128px;
    background: #f8f8f8;
    left: 0;
	height: calc(100% - 72px);
	padding: 5px;
	z-index: 10000;
	
	display: flex;
    flex-direction: column;
    justify-content: flex-start;
	align-items: center;
	padding-top: 10px;

	overflow: hidden;
	overflow-y: scroll;
	
	-ms-overflow-style: none;
}

.taskbar::-webkit-scrollbar {
	display: none;
}

.taskbar__task {
	background: transparent;	
	cursor: pointer;
	display: flex;
	align-items: center;
	width: 100%;
	padding: 5px;
    border: 1px solid #e2e2e2;
    margin-bottom: 2px;
	min-height: 32px;
}

.taskbar__task-icon {
	margin-left: auto;
}

.taskbar__task svg {
	flex: 0 0 auto;
    justify-content: flex-end;
    margin-left: auto;
}

.taskbar__taskname {
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
	flex: 0 1 auto;
	font-size: 12px;
}
.taskbar__task-text {
	fill: #000000;
}

.taskbar__task-circle {
	fill: #ffffff;
	stroke-width: 2;
	stroke: #000000;
}


.ui-controls-bar {
	position: fixed;
    top: 72px;
    width: 150px;
    background: #000000;
    right: 0;
    height: calc(100% - 127px);
	z-index: 10000;
	
	display: flex;
    flex-direction: column;
    justify-content: flex-start;
	align-items: center;
	padding-top: 10px;

	overflow: hidden;
	overflow-y: scroll;
	
	-ms-overflow-style: none;
	padding: 10px;
	padding-left: 5px;
	padding-right: 15px;
}

.ui-controls-bar__small-width {
	width: 48px;
	overflow: visible;
}

.MuiSlider-valueLabel {
	transform: translateY(9px) translateX(-20px) rotate(-90deg) !important;
	z-index: 100000;
}

.MuiSlider-valueLabel span span {
	transform: rotate(135deg);
}

.ui-controls-bar::-webkit-scrollbar {
	display: none;
}

.ui-controls-bar__debug-info {
	white-space: pre-wrap;
	color: white;
	font-size: 12px;
	overflow-x: scroll;
	width: 100%;
	height: 100%;
}

.ui-controls-bar__debug-info::-webkit-scrollbar {
	display: none;
}

.ui-controls-bar__debug-info-content {
	width: auto;
    display: block;
    white-space: pre;
}


.debug-info {
	position: fixed;
    bottom: 10px;
    width: 150px;
    background: #000000;
    right: 10px;
    height: 200px;
	z-index: 9990;
	
	display: flex;
    flex-direction: column;
    justify-content: flex-start;
	align-items: center;
	padding-top: 10px;

	overflow: hidden;
	overflow-y: scroll;
	
	-ms-overflow-style: none;
	padding: 10px;
	padding-left: 5px;
	padding-right: 15px;
}

.debug-info--fullscreen {
	width: calc(100% - 40px);
	height: calc(100% - 40px);
	right: auto;
	left: 20px;
	top: 20px;
	bottom: auto;
	z-index: 20000;
	padding: 20px;
}

.debug-info::-webkit-scrollbar {
	display: none;
}

.debug-info__debug-info {
	white-space: pre-wrap;
	color: white;
	font-size: 12px;
	overflow-x: scroll;
	width: 100%;
	height: 100%;
}

.debug-info__debug-info::-webkit-scrollbar {
	display: none;
}

.debug-info__debug-info-content {
	width: auto;
    display: block;
    white-space: pre;
}

@media only screen and (max-width: 1023px) {
	.debug-info:not(.debug-info--fullscreen) {
		height: 20px;
		width: 50px;
	}

	.debug-info__debug-info-content {
		display: none;
	}

	.debug-info--fullscreen .debug-info__debug-info-content {
		display: block;
	}
}

input.form-control,
textarea.form-control {
