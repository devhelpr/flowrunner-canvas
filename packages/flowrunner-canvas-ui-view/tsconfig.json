{
	"compilerOptions": {
	  "jsx":"react-jsx",
	  "target": "es2018",
	  "noImplicitAny": false,
	  "module": "esnext",
	  "removeComments": true,
	  "moduleResolution":"node",
	  "outDir": "./lib",
	  "strict": true,
	  "allowJs": true,
	  "esModuleInterop": true,
	  "sourceMap": true,
	  "declaration": true,
	  "skipLibCheck": true
	},
	"include": ["src"],
	"exclude": ["node_modules", "**/__tests__/*"]
  }