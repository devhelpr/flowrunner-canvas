{
	"compilerOptions": {
	  "jsx":"react",
	  "target": "es2015",
	  "noImplicitAny": false,
	  "module": "esnext",
	  "removeComments": true,
	  "moduleResolution":"node",
	  "outDir": "./lib",
	  "strict": true,
	  "allowJs": true,
	  "lib": ["dom", "es2015", "es2015.promise"],
	  "esModuleInterop": true
	},
	"include": ["src"],
	"exclude": ["node_modules", "**/__tests__/*"]
  }