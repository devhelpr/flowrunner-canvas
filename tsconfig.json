{
	"compilerOptions": {
	  "jsx":"react",
	  "target": "esnext",
	  "lib": ["DOM", "ES6", "DOM.Iterable", "ScriptHost", "ES2016.Array.Include"],
	  "noImplicitAny": false,
	  "module": "esnext",
	  "removeComments": true,
	  "moduleResolution":"node",
	  "outDir": "./lib",
	  "strict": true,
	  "allowJs": true,
	  "esModuleInterop": true,
	  "sourceMap": true,
	  "declaration": true,
	  "skipLibCheck": true
	},
	"include": ["src"],
	"exclude": ["node_modules", "**/__tests__/*"]
  }